# Streams Prefetcher - Environment Variables
# Copy this file to .env and configure your values

# Required: Hostname for accessing the web interface
STREAMS_PREFETCHER_HOSTNAME=streams-prefetcher.yourdomain.com

# Required: Docker data directory (should match your existing setup)
# This is where configuration, database, and logs will be stored
DOCKER_DATA_DIR=/opt/docker/data

# Optional: Timezone (defaults to UTC)
TZ=UTC
